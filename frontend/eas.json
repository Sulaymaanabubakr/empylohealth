{
    "cli": {
        "version": ">= 5.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": false
            },
            "android": {
                "gradleCommand": ":app:assembleDebug"
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": false
            },
            "android": {
                "buildType": "apk",
                "gradleCommand": ":app:assembleRelease"
            }
        },
        "production": {
            "ios": {
                "simulator": false,
                "autoIncrement": "buildNumber"
            },
            "android": {
                "buildType": "app-bundle",
                "autoIncrement": "versionCode"
            }
        }
    },
    "submit": {
        "production": {
            "ios": {
                "appleId": "fredr7457@gmail.com",
                "ascAppId": "126937675",
                "bundleIdentifier": "com.empylo.circlesapp"
            }
        }
    }
}
